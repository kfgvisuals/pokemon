<p id="notice"><%= notice %></p>

<h1>Pokebags</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pokebags.each do |pokebag| %>
      <tr>
        <td><%= link_to 'Show', pokebag %></td>
        <td><%= link_to 'Edit', edit_pokebag_path(pokebag) %></td>
        <td><%= link_to 'Destroy', pokebag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pokebag', new_pokebag_path %>
